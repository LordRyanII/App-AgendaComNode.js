<!--ALERTA-->

<!--Erro-->
<% if(errors.length > 0) { %>
    <div class="col my-3">
        <div class="alert alert-danger">
            <% errors.forEach(erro => { %>
                <%= erro %> <br>
            <% }); %>
        </div>
    </div>
<% } %>

<!--Sucesso-->
<% if(success.length > 0) { %>
    <div class="col my-3">
    <div class="alert alert-success">
        <% success.forEach(msg => { %>
            <%= msg %> <br>
        <% }); %>
    </div>
</div>
<% } %>
